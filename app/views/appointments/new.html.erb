<div class="container">
  <div class="row">
    <div class="col-12">
      <% @appointments.each do |appointment| %>
        <p><%= appointment.date %> </p>
        <p><%= appointment.user.first_name %> | <%= appointment.user.last_name %> </p>
      <% end %>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-12">
      <%= simple_form_for [@doctor, @appointment] do |f| %>
        <%= f.datetime_select  :date  %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
